{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": false,
    "target": "es2017",
    "types": [
      "jest"
    ],
    "resolveJsonModule": true
  },
  "include": [
    "src/index.ts",
    "src/aws_iframe_extension.ts",
    "src/MainLauncher.ts",
    "src/LeftSideLauncher.ts",
    "src/constants.ts",
    "src/client.ts",
    "src/types.ts",
    "src/domain.json"
  ]
}
